<%= form_for([@organization, @organization.locations.build]) do |f| %>

    <div class="field">
      <%= f.label :address %>
      <%= f.text_field :address %>
    </div>

    <div class="field">
      <%= f.label :state %>
      <%= f.text_field :state %>
    </div>

    <div class="field">
      <%= f.label :zipcode %>
      <%= f.number_field :zipcode %>
    </div>

    <div class="field">
      <%= f.label :city %>
      <%= f.text_field :city %>
    </div>

    <div class="field">
      <%= f.label :org_name %>
      <%= f.text_field :org_name %>
    </div>

    <div class="actions">
      <%= f.submit %>
    </div>
<% end %>