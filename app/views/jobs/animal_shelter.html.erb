<% provide(:title, "Animal Shelter Jobs") %>
<p id="notice"><%= notice %></p>

<h1>Animal Shelter Jobs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Text</th>
      <th>Date</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.title %></td>
        <td><%= job.text %></td>
        <td><%= job.date %></td>
        <td><%= job.job_type %></td>
        <td><%= link_to 'Show', job %></td>
        <td><%= link_to 'Edit', edit_job_path(job) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

